{
  "metadata": {
    "title": "BarcelonaJS",
    "description": "monthly JavaScript meetup in Barcelona"
  },
  "plugins": {
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":title"
    },
    "metalsmith-layouts": {
      "engine": "handlebars",
      "partials": "partials"
    },
    "metalsmith-assets": {
      "source": "./assets",
      "destination": "./assets"
    }
  }
}
